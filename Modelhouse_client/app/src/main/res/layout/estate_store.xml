                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="거래유형" />

                        <RadioGroup
                            android:id="@+id/price_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/trade"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="매매" />

                            <RadioButton
                                android:id="@+id/lent"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="전세" />

                            <RadioButton
                                android:id="@+id/monthly"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="임대" />
                        </RadioGroup>
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

