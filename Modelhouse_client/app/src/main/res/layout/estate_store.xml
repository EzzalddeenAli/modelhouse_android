                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="융자금" />

                        <EditText
                            android:id="@+id/loan"
                            android:layout_width="fill_parent"
                            android:layout_height="65px"
                            android:background="@drawable/border" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lv_floors"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="건물층수" />


                        <EditText
                            android:id="@+id/total_floor"
                            android:layout_width="fill_parent"
                            android:layout_height="65px"
                            android:layout_weight="1"
                            android:background="@drawable/border"
                            android:hint="전체층" />

                        <EditText
                            android:id="@+id/floor"
                            android:layout_width="fill_parent"
                            android:layout_height="65px"
                            android:layout_weight="1"
                            android:background="@drawable/border"
                            android:hint="해당층" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lv_heater"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="난방방식" />

                        <EditText
                            android:id="@+id/heater"
                            android:layout_width="fill_parent"
                            android:layout_height="65px"
                            android:background="@drawable/border" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lv_fuel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="난방연료" />

                        <EditText
                            android:id="@+id/fuel"
                            android:layout_width="fill_parent"
                            android:layout_height="65px"
                            android:background="@drawable/border" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lv_complete"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="준공년월" />

                        <EditText
                            android:id="@+id/complete"
                            android:layout_width="fill_parent"
                            android:layout_height="65px"
                            android:background="@drawable/border" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lv_parking"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="주차대수" />

                        <EditText
                            android:id="@+id/parking"
                            android:layout_width="fill_parent"
                            android:layout_height="65px"
                            android:background="@drawable/border" />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>


            <Button
                android:id="@+id/submit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="매물 등록하기" />

        </LinearLayout>
    </ScrollView>

</LinearLayout>